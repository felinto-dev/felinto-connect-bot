{
	"name": "@felinto-dev/felinto-connect-bot",
	"version": "2.4.0",
	"main": "lib/dist/index.js",
	"module": "lib/dist/index.mjs",
	"types": "lib/dist/index.d.ts",
	"private": false,
	"license": "MIT",
	"scripts": {
		"dev": "cd lib && pnpm dev",
		"build": "cd lib && pnpm build",
		"test": "cd lib && pnpm test",
		"lint": "cd lib && pnpm lint",
		"release": "pnpm run build && changeset publish",
		"clean": "rm -rf lib/dist node_modules playground/node_modules playground/backend/node_modules playground/frontend/node_modules",
		"setup": "pnpm install && pnpm playground:install",
		"playground:install": "cd playground && pnpm install && cd backend && pnpm install && cd ../frontend && pnpm install",
		"playground:dev": "cd playground && pnpm dev",
		"playground:backend": "cd playground/backend && pnpm dev",
		"playground:frontend": "cd playground/frontend && pnpm dev",
		"playground:server": "cd playground/backend && pnpm start",
		"lib:build": "pnpm build",
		"lib:dev": "pnpm dev",
		"lib:test": "pnpm test",
		"lib:install": "cd lib && pnpm install",
		"lib:publish": "cd lib && npm publish"
	},
	"devDependencies": {
		"@changesets/cli": "^2.27.9",
		"@anthropic-ai/claude-code": "latest"
	},
	"pnpm": {
		"overrides": {
			"puppeteer": "^22.14.0",
			"puppeteer-core": "^22.14.0"
		}
	}
}