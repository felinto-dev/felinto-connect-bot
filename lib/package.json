		"jest": "^29.7.0",
		"ts-jest": "^29.2.5",
		"tsup": "^8.3.5",
		"typescript": "^5.7.2",
		"puppeteer": "^22.15.0"
	},
	"dependencies": {
		"puppeteer-extra": "^3.3.6",
		"puppeteer-extra-plugin-recaptcha": "^3.6.8",
